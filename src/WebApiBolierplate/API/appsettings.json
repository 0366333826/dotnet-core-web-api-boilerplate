{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "DataBase": {
      "ConnectionString": "This is a database connection string"
    },
    "Logger": {
      "DateFormat": "yyyy-MM-dd HH:mm:ss.fff",
      "FileName": "ErrorLog"
    },
    "JWT": {
      "Audience": "https://www.example.com",
      "Issuer": "WebApiBoilerplate",
      "Key": "This is a JWT secret key"
    }
  }
}
